programa {
  funcao inicio() {
    
      real resposta
      escreva("Quantos chocolates você pretende comer na vida?")

      se(resposta <= 1000){
        escreva("Bom, na vida inteira... Até que é pouco")
      }

      se(resposta > 1000){
        escreva("Isso é bastante! se controle!")
      }

  }

}
